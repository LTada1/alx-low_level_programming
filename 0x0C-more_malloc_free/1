#include "main.h"
#include <stdio.h>
#include <string.h>
#include <stdlib.h>

/**
 * string_nconcat - concatenates two strings
 * @n: number of bytes
 * @s1: string variable
 * @s2: string variable
 *
 * Return: pointer.
 */

void *_calloc(unsigned int nmemb, unsigned int size)
{
unsigned int i;
int *ptr;

if (nmemb == 0 || size == 0)
{
return (NULL);
}
for (i = 0; i < size; i++)
{
ptr = (int **)malloc(sizeof(size) * nmemb);
}
}
if (ptr == NULL)
{
return (NULL);
}
for (i = 0; i < size; i++)
{
ptr[i]= 0;
}
}
return (ptr);
}
